<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SetUp">

    <TextView
        android:id="@+id/return_login"
        android:layout_width="182dp"
        android:layout_height="31dp"

        android:layout_marginTop="23dp"
        android:onClick="backLogin"
        android:text="Return to login screen"
        android:textColor="#2196F3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.885"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_Clr"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="306dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="41dp"
        android:text="Setup"
        android:textAlignment="center"
        android:textColor="@color/blue_50"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" /><![CDATA[
    #app:layout_constraintStart_toStartOf="@+id/apiClientId"
        #app:layout_constraintTop_toBottomOf="@+id/apiClientId" />
    #app:layout_constraintStart_toStartOf="@+id/apiClientId"
        #app:layout_constraintTop_toBottomOf="@+id/apiClientId" />









    ]]>

    <Button
        android:id="@+id/button_Clr"
        android:layout_width="117dp"
        android:layout_height="55dp"

        android:layout_marginEnd="4dp"
        android:layout_marginBottom="104dp"
        android:text="Clear"
        android:textColor="@color/white"
        android:theme="@style/PrimaryButton"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/TILapiClientSecret" />

    <Button
        android:id="@+id/get_quote"
        android:layout_width="117dp"
        android:layout_height="55dp"

        android:layout_marginEnd="16dp"
        android:layout_marginBottom="106dp"
        android:text="Save"
        android:textColor="@color/white"
        android:theme="@style/PrimaryButton"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button_Clr"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/TILapiClientSecret"
        app:layout_constraintTop_toTopOf="@+id/button_Clr"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="70dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="71dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ins_logo" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/TILbaseVerifyURL"
        android:layout_width="wrap_content"
        android:layout_height="51dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintEnd_toEndOf="@+id/textView"
        app:layout_constraintHorizontal_bias="0.384"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:passwordToggleEnabled="true">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/first_name"
            android:layout_width="wrap_content"
            android:layout_height="51dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="36dp"
            android:layout_marginEnd="2dp"
            android:backgroundTint="@android:color/darker_gray"
            android:cursorVisible="true"
            android:ems="10"
            android:hint="baseVerifyURL"
            android:inputType="textPassword"
            android:shadowColor="@android:color/darker_gray"
            android:singleLine="true"
            android:textAlignment="textStart"
            android:textColor="@color/blue_50"
            android:textColorHint="@color/blue_50"
            android:textColorLink="@color/blue_50"
            android:textIsSelectable="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView" />

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/TILappClientId"
        android:layout_width="wrap_content"
        android:layout_height="51dp"
        android:layout_marginTop="104dp"
        app:layout_constraintEnd_toEndOf="@+id/TILbaseVerifyURL"
        app:layout_constraintStart_toStartOf="@+id/TILbaseVerifyURL"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:passwordToggleEnabled="true">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/email_id"
            android:layout_width="wrap_content"
            android:layout_height="51dp"
            android:layout_marginTop="148dp"
            android:backgroundTint="@android:color/darker_gray"
            android:cursorVisible="true"
            android:ems="10"
            android:hint="appClientId"
            android:inputType="textPassword"
            android:shadowColor="@android:color/darker_gray"
            android:singleLine="true"
            android:textAlignment="textStart"
            android:textColor="@color/blue_50"
            android:textColorHint="@color/blue_50"
            android:textColorLink="@color/blue_50"
            android:textIsSelectable="true" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/TILappClientSecret"
        android:layout_width="wrap_content"
        android:layout_height="51dp"
        android:layout_marginTop="184dp"
        app:layout_constraintEnd_toEndOf="@+id/TILappClientId"
        app:layout_constraintStart_toStartOf="@+id/TILappClientId"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:passwordToggleEnabled="true">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/last_name"
            android:layout_width="wrap_content"
            android:layout_height="51dp"
            android:backgroundTint="@android:color/darker_gray"
            android:cursorVisible="true"
            android:ems="10"
            android:hint="appClientSecret"
            android:inputType="textPassword"
            android:shadowColor="@android:color/darker_gray"
            android:singleLine="true"
            android:textAlignment="textStart"
            android:textColor="@color/blue_50"
            android:textColorHint="@color/blue_50"
            android:textColorLink="@color/blue_50"
            android:textIsSelectable="true" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/TILapiClientId"
        android:layout_width="wrap_content"
        android:layout_height="51dp"
        android:layout_marginTop="264dp"
        app:layout_constraintEnd_toEndOf="@+id/TILappClientSecret"
        app:layout_constraintStart_toStartOf="@+id/TILappClientSecret"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:passwordToggleEnabled="true">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/apiClientId"
            android:layout_width="wrap_content"
            android:layout_height="51dp"
            android:backgroundTint="@android:color/darker_gray"
            android:cursorVisible="true"
            android:ems="10"
            android:hint="apiClientId"
            android:inputType="textPassword"
            android:shadowColor="@android:color/darker_gray"
            android:singleLine="true"
            android:textAlignment="textStart"
            android:textColor="@color/blue_50"
            android:textColorHint="@color/blue_50"
            android:textColorLink="@color/blue_50"
            android:textIsSelectable="true" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/TILapiClientSecret"
        android:layout_width="wrap_content"
        android:layout_height="51dp"
        android:layout_marginTop="340dp"
        app:layout_constraintEnd_toEndOf="@+id/TILapiClientId"
        app:layout_constraintStart_toStartOf="@+id/TILapiClientId"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:passwordToggleEnabled="true">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/apiClientSecret"
            android:layout_width="wrap_content"
            android:layout_height="51dp"
            android:backgroundTint="@android:color/darker_gray"
            android:cursorVisible="true"
            android:ems="10"
            android:hint="apiClientSecret"
            android:inputType="textPassword"
            android:shadowColor="@android:color/darker_gray"
            android:singleLine="true"
            android:textAlignment="textStart"
            android:textColor="@color/blue_50"
            android:textColorHint="@color/blue_50"
            android:textColorLink="@color/blue_50"
            android:textIsSelectable="true" />
    </com.google.android.material.textfield.TextInputLayout>


</androidx.constraintlayout.widget.ConstraintLayout>
